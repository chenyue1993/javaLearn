<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>f</servlet-name>
  	<servlet-class>com.chenyue.servlet.Forward</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>f</servlet-name>
  	<url-pattern>/f</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>r</servlet-name>
  	<servlet-class>com.chenyue.servlet.Redirect</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>r</servlet-name>
  	<url-pattern>/r</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>s</servlet-name>
  	<servlet-class>com.chenyue.servlet.Save</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>s</servlet-name>
  	<url-pattern>/save</url-pattern>
  </servlet-mapping>
</web-app>
