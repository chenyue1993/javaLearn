<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>regist.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>httpservletrequest</servlet-name>
  	<servlet-class>com.chenyue.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>httpservletrequest</servlet-name>
  	<url-pattern>/request</url-pattern>
  </servlet-mapping>
   <servlet>
  	<servlet-name>hrequest</servlet-name>
  	<servlet-class>com.chenyue.servlet.Save</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>hrequest</servlet-name>
  	<url-pattern>/user/save</url-pattern>
  </servlet-mapping>
</web-app>