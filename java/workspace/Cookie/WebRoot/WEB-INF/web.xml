<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>cookie</servlet-name>
  	<servlet-class>com.chenyue.servlet.CreateAndSendCookie</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>cookie</servlet-name>
  	<url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>receiveCookie</servlet-name>
  	<servlet-class>com.chenyue.servlet.ReceiveCookie</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>receiveCookie</servlet-name>
  	<url-pattern>/receivecookie</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.chenyue.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>welcome</servlet-name>
  	<servlet-class>com.chenyue.servlet.Welcome</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>welcome</servlet-name>
  	<url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>error</servlet-name>
  	<servlet-class>com.chenyue.servlet.Error</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>error</servlet-name>
  	<url-pattern>/error</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>save</servlet-name>
  	<servlet-class>com.chenyue.servlet.Save</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>save</servlet-name>
  	<url-pattern>/save</url-pattern>
  </servlet-mapping>
</web-app>
