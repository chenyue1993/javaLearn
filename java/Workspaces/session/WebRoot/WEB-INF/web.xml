<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>setsession</servlet-name>
  	<servlet-class>com.chenyue.servlet.SetSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>setsession</servlet-name>
  	<url-pattern>/setsession</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>getsession</servlet-name>
  	<servlet-class>com.chenyue.servlet.GetSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>getsession</servlet-name>
  	<url-pattern>/getsession</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>logoutsession</servlet-name>
  	<servlet-class>com.chenyue.servlet.LogoutSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>logoutsession</servlet-name>
  	<url-pattern>/logoutsession</url-pattern>
  </servlet-mapping>
</web-app>
