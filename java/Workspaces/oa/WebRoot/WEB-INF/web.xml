<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>save</servlet-name>
  	<servlet-class>com.chenyue.web.SaveCustomer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>save</servlet-name>
	<url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>list</servlet-name>
  	<servlet-class>com.chenyue.web.ListCustomer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>list</servlet-name>
	<url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>delete</servlet-name>
  	<servlet-class>com.chenyue.web.DeleteCustomer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>delete</servlet-name>
	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>detail</servlet-name>
  	<servlet-class>com.chenyue.web.DetailCustomer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>detail</servlet-name>
	<url-pattern>/detail</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>modify</servlet-name>
  	<servlet-class>com.chenyue.web.ModifyCustomer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>modify</servlet-name>
	<url-pattern>/modify</url-pattern>
  </servlet-mapping>
</web-app>
